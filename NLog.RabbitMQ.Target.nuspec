			<dependency id="NLog" version="4.5.0" exclude="Build,Analyzers" />
			<dependency id="Newtonsoft.Json" version="10.0.3" exclude="Build,Analyzers" />
			<dependency id="RabbitMQ.Client" version="5.0.1" exclude="Build,Analyzers" />
		  </group>
		</dependencies>
    </metadata>
    <files>
       <file src="src\Nlog.RabbitMQ.Target\bin\Release\**\*.*" target="lib"/>
